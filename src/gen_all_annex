#! /usr/bin/env bash

lang="$1"
paper="$2"

case "$lang" in
	"en")
		case "$paper" in
			"bonner")
				./src/gen_latex_table \
					data/converted/bonner_82_arginine.csv \
					arginine

				echo

				./src/gen_latex_table \
					data/converted/bonner_82_lysine.csv \
					lysine
				;;
			"ellerton")
				./src/gen_latex_table \
					data/converted/ellerton_64_alpha_amino_n_butyric_acid.csv \
