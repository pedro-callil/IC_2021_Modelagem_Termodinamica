					data/converted/himanshu_05_dl_methionine.csv \
					methionine

				echo

				./src/gen_latex_table \
					data/converted/himanshu_05_dl_valine.csv \
					valine

				echo

				./src/gen_latex_table \
					data/converted/himanshu_05_glycine.csv \
					glycine

				echo

				./src/gen_latex_table \
					data/converted/himanshu_05_hydroxy_l_proline.csv \
					hidroxyproline

				echo

