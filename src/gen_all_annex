				echo
				./src/gen_latex_table \
					data/converted/maximo_10_d_fructose_46_7_kPa.csv \
					fructose/46.7kPa
				echo
				./src/gen_latex_table \
					data/converted/maximo_10_d_fructose_53_3_kPa.csv \
					fructose/53.3kPa
				echo
				./src/gen_latex_table \
					data/converted/maximo_10_d_fructose_60_0_kPa.csv \
					fructose/60.0kPa
				echo
				./src/gen_latex_table \
					data/converted/maximo_10_d_fructose_66_7_kPa.csv \
