				echo

				./src/gen_latex_table \
					data/converted/ellerton_64_glycine.csv \
					glycine

				echo

				./src/gen_latex_table \
					data/converted/ellerton_64_glycylglycine.csv \
					glycylglycine

				echo

				./src/gen_latex_table \
					data/converted/ellerton_64_lactamide.csv \
					lactamide

				echo

				./src/gen_latex_table \
					data/converted/ellerton_64_valine.csv \
					valine
